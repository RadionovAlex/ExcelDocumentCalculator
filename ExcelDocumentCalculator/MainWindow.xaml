<Window x:Class="ExcelDocumentCalculator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ExcelDocumentCalculator"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800"
        Background="Beige">
    <Grid>
        <TextBox Text="Sofiia calculator" Height="50" TextAlignment="Justify" FontSize="40" VerticalAlignment="Top" HorizontalAlignment="Center"/>
        <Button Margin="100,150,100,150" FontSize="40" Click="Button_Click">Choose file</Button>

    </Grid>
</Window>
